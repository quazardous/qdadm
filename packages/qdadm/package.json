{
  "name": "qdadm",
  "version": "1.1.5",
  "description": "Vue 3 framework for admin dashboards with PrimeVue",
  "author": "quazardous",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quazardous/qdadm.git"
  },
  "homepage": "https://github.com/quazardous/qdadm#readme",
  "bugs": {
    "url": "https://github.com/quazardous/qdadm/issues"
  },
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types"
  },
  "exports": {
    ".": "./src/index.ts",
    "./auth": "./src/auth/index.ts",
    "./security": "./src/security/index.ts",
    "./composables": "./src/composables/index.ts",
    "./components": "./src/components/index.ts",
    "./editors": "./src/editors/index.ts",
    "./module": "./src/module/index.ts",
    "./utils": "./src/utils/index.ts",
    "./modules/debug": "./src/modules/debug/index.ts",
    "./styles": "./src/styles/index.scss",
    "./styles/variables": "./src/styles/_variables.scss",
    "./gen": "./src/gen/index.ts",
    "./gen/vite-plugin": "./src/gen/vite-plugin.ts"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@quazardous/quarkernel": "^2.1.0",
    "pluralize": "^8.0.0"
  },
  "peerDependencies": {
    "pinia": "^2.0.0",
    "primevue": "^4.0.0",
    "vue": "^3.3.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vanilla-jsoneditor": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "vanilla-jsoneditor": "^0.23.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "admin",
    "dashboard",
    "framework",
    "primevue",
    "crud",
    "entity-manager"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.6",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^17.0.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.7.4",
    "sass": "^1.97.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^3.2.2"
  }
}
